{
    "eslint.enable": false,
    "jshint.enable": false,
    "tslint.enable": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    }
}
